gunit Date;
@header{package com.natty.parse;}

explicit_time:
"10:00am" -> (EXPLICIT_TIME 10 00 am)
"10a" -> (EXPLICIT_TIME 10 0 am)
"10am" -> (EXPLICIT_TIME 10 0 am)
"10" -> (EXPLICIT_TIME 10 0)
"8p" -> (EXPLICIT_TIME 8 0 pm)
