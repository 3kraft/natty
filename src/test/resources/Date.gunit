"8 pm" -> (EXPLICIT_TIME 8 0 pm)
"noon" -> (EXPLICIT_TIME 12 0 pm)
"midnight" -> (EXPLICIT_TIME 12 0 am)

relative_date:
"yesterday" -> (RELATIVE_DATE < 1)
"day before yesterday" -> (RELATIVE_DATE < 2)
"the day before yesterday" -> (RELATIVE_DATE < 2)
"tomorrow" ->(RELATIVE_DATE > 1)
"day after tomorrow" ->(RELATIVE_DATE > 2)
"the day after tomorrow" ->(RELATIVE_DATE > 2)
"in 3 days" ->(RELATIVE_DATE > by_day 3 day)
"3 days ago" ->(RELATIVE_DATE < by_day 3 day)
"in 3 weeks" ->(RELATIVE_DATE > by_day 3 week)
"four weeks ago" ->(RELATIVE_DATE < by_day 4 week)
"in 3 months" ->(RELATIVE_DATE > by_day 3 month)
"three months ago" ->(RELATIVE_DATE < by_day 3 month)
"in 3 years" ->(RELATIVE_DATE > by_day 3 year)
"seven years ago" ->(RELATIVE_DATE < by_day 7 year)
"next monday" -> (RELATIVE_DATE > by_week 1 2)
"next mon" -> (RELATIVE_DATE > by_week 1 2)
"4 mondays from now" -> (RELATIVE_DATE > by_day 4 2)
"next weekend" -> (RELATIVE_DATE > by_week 1 7)
"six mondays ago" -> (RELATIVE_DATE < by_day 6 2)
"last monday" -> (RELATIVE_DATE < by_week 1 2)
"last mon" -> (RELATIVE_DATE < by_week 1 2)
"this past mon" -> (RELATIVE_DATE < by_day 1 2)
"this coming mon" -> (RELATIVE_DATE > by_day 1 2)
"this upcoming mon" -> (RELATIVE_DATE > by_day 1 2)
"next thurs" -> (RELATIVE_DATE > by_week 1 5)
"next month" -> (RELATIVE_DATE > by_week 1 month)
"last month" -> (RELATIVE_DATE < by_week 1 month)
"next week" -> (RELATIVE_DATE > by_week 1 week)
"last week" -> (RELATIVE_DATE < by_week 1 week)
"next year" -> (RELATIVE_DATE > by_week 1 year)
"last year" -> (RELATIVE_DATE < by_week 1 year)

